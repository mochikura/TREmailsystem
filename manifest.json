{
    "name":"FileTRE test",
    "version":"0.1",
    "description":"File Time Release Encryption for FUN mail system!",
    "manifest_version":2,
    "content_scripts":[
        {
            "matches":[
                "https://webmail.fun.ac.jp/cgi-bin/htmlparse.cgi?html=composerframe.html"
            ],
            "js":[
                "fileget.js",
                "initenc.js",
                "mcl.js",
                "index.js",
                "enc.js",
                "getkey.js",
                "axios.js",
                "js/crypto-js.js",
                "js/aes.js"

            ],
            "run_at": "document_end",
            "all_frames": true
        },{
            "matches":[
            "https://webmail.fun.ac.jp/cgi-bin/htmlparse.cgi?html=frame.html"
        ],
        "js":[
            "index.js",
            "axios.js",
            "mcl.js",
            "getkey.js",
            "dec.js",
            "js/aes.js",
            "js/crypto-js.js",
            "fileget.js",
            "initdec.js"
        ],
        "run_at":"document_end",
        "all_frmes":true
    }
    ],
    "browser_action":{
        "default_popup":"popup.html"
    },
    "web_accessible_resources":["fileget.js"],
    "content_security_policy":"script-src 'self' 'wasm-eval' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com/* https://cdn.jsdelivr.net; object-src 'self'"
}